{
  "kind": "collectionType",
  "collectionName": "team_members",
  "info": {
    "singularName": "team-member",
    "pluralName": "team-members",
    "displayName": "Team Member"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "image": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "name": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "designation": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "priority": {
      "type": "decimal",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "default": 0,
      "min": 0,
      "max": 100
    },
    "languages": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::language.language"
    },
    "social_link": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "elements.logo-link",
      "repeatable": true
    }
  }
}
